local connections = {}

function create_connection(script_signal, callback)
    local proxy = {
        connection = script_signal:Connect(callback)
    }
    
    function proxy:disconnect()
        self.connection:Disconnect()

        table.clear(self)
        self = nil
    end

    table.insert(connections, proxy)
    return proxy
end

